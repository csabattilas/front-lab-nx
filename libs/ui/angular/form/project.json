{
  "name": "ng-form",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/angular/form/src",
  "prefix": "fl-form",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/ui/angular/form",
        "config": "libs/ui/angular/form/vite.config.mts"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "libs/ui/angular/form/vite.config.mts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/ui/angular/form/**/*.ts",
          "libs/ui/angular/form/**/*.html"
        ]
      }
    }
  }
}
